{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
  "hosting": {
    "rewrites": [ {
      "source": "/onlinerest",
      "run": {
        "serviceId": "onlinerest",  // "service name" (from when you deployed the container image)
        "region": "asia-southeast1"
      }
    } ]
  }
}
